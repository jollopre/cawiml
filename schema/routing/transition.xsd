<?xml version="1.0" encoding="UTF-8"?>
<xs:schema 
    targetNamespace="http://www.surveysm.com"
    xmlns:s="http://www.surveysm.com"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    elementFormDefault="qualified">
    <xs:complexType name="Transition">
        <xs:annotation>
            <xs:documentation>
                It connects the current state with the target state specified and describes the flow between states.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="target" use="required" type="xs:string"></xs:attribute>
    </xs:complexType>
</xs:schema>
