<?xml version="1.0" encoding="UTF-8"?>
<xs:schema 
    targetNamespace="https://github.com/jollopre/ssm"
    xmlns:s="https://github.com/jollopre/ssm"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    elementFormDefault="qualified">
    <xs:include schemaLocation="rpn.xsd"></xs:include>
    <xs:complexType name="Computation">
        <xs:choice>
            <xs:element name="assignment" type="s:Assignment"></xs:element>
        </xs:choice>
        <xs:attribute name="ref" type="xs:string" use="required"></xs:attribute>           
    </xs:complexType>
</xs:schema>
