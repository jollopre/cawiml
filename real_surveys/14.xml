                <close code="10">
                    <label lang="en">Restaurant Manager</label>
                </close>
                <open code="11">
                    <label lang="en">Others (WRITE IN)</label>
                    <string>
                        <value></value>
                    </string>
                </open>
            </multiple>
            <multiple name="Q12">
