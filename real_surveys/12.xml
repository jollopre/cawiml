                <transition target="p1"/>
            </state>
            <state id="p1">
                <if>
                    <condition>
                        <multivariable ref="Q13" row="1"/>
                        <constant type="string" value="2"/>
                        <operator name="IS_SEL"/>
                        <multivariable ref="Q13" row="2"/>
                        <constant type="string" value="2"/>
                        <operator name="IS_SEL"/>
                        <operator name="OR"/>
                        <multivariable ref="Q13" row="3"/>
                        <constant type="string" value="2"/>
                        <operator name="IS_SEL"/>
                        <multivariable ref="Q13" row="4"/>
