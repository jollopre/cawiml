            <state id="s17">
                <variable ref="Q17"/>
                <transition target="s18"/>
            </state>
            <state id="s18">
                <variable ref="Q18"/>
                <transition target="s19"/>
            </state>
            <state id="s19">
                <variable ref="Q19"/>
                <transition target="sink0"/>
            </state>
