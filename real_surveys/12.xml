                        <operator name="IS_SEL"/>
                        <operator name="OR"/>
                    </condition>
                    <then>
                        <transition target="s17"/>
                    </then>
                    <else>
                        <transition target="s16"/>
                    </else>
                </if>
            </state>
            <state id="s16">
                <variable ref="Q16"/>
