                        <constant type="string" value="99"/>
                        <operator name="IS_SEL"/>
                        <operator name="OR"/>
                    </condition>
                    <then>
                        <transition target="terminate0"/>
                    </then>
                    <else>
                        <transition target="s5"/>
                    </else>
                </if>
            </state>
            <state id="s5">
                <variable ref="S5"/>
                <transition target="sink0"/>
            </state>
            <state id="sink0">
                <sink/>
            </state>
            <state id="terminate0">
                <terminate/>
            </state>
        </statemodel>
        <statemodel ref="1">
            <source id="s1"/>
            <state id="s1">
                <variable ref="Q1"/>
                <transition target="s2"/>
            </state>
            <state id="s2">
                <variable ref="Q2"/>
                <transition target="p0"/>
            </state>
            <state id="p0">
