                    <condition>
                        <variable ref="Q2"/>
                        <constant type="string" value="1"/>
                        <operator name="IS_SEL"/>
                    </condition>
                    <then>
                        <transition target="s3_1"/>
                    </then>
                    <else>
                        <transition target="p1"/>
                    </else>
                </if>
            </state>
            <state id="s3_1">
                <variable ref="Q3_1"/>
                <transition target="p2"/>
            </state>
            <state id="p2">
                <for>
                    <field ref="p2_iterator"/>
                    <in>
                        <expr_list>
                            <variable ref="Q3_1"/>
