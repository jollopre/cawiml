                        </close>
                        <close code="6">
                            <label lang="en">6</label>
                            <label lang="es">6</label>
                        </close>
                        <close code="7">
                            <label lang="en">7</label>
                            <label lang="es">7</label>
                        </close>
                    </columns>
                </single>
            </grid>
            <intro name="END">
                <label lang="en">Thank and close</label>
                <label lang="es">Gracias y cerrar</label>
            </intro>
        </section>
    </content>
    <field></field>
    <routing>
        <statemodel ref="DEFAULT">
            <source id="s0"/>
            <state id="s0">
                <variable ref="START"/>
                <variable ref="QA"/>
                <variable ref="QB"/>
                <variable ref="QC"/>
                <transition target="s1"/>
            </state>
            <state id="s1">
                <variable ref="Q1"/>
                <transition target="s2"/>
            </state>
            <state id="s2">
                <variable ref="Q2"/>
                <transition target="p0"/>
            </state>
            <state id="p0">
                <if>
                    <condition>
                        <variable ref="Q1"/>
                        <constant type="string" value="yes"/>
                        <operator name="IS_SEL"/>
                    </condition>
                    <then>
                        <transition target="s3"/>
                    </then>
                    <else>
                        <transition target="p1"/>
                    </else>
                </if>
            </state>
            <state id="s3">
                <variable ref="Q3"/>
                <transition target="p1"/>
            </state>
            <state id="p1">
                <if>
                    <condition>
                        <variable ref="Q3"/>
                        <constant type="string" value="1"/>
                        <operator name="IS_SEL"/>
                        <variable ref="Q3"/>
                        <constant type="string" value="2"/>
                        <operator name="IS_SEL"/>
                        <operator name="OR"/>
                        <variable ref="Q1"/>
                        <constant type="string" value="no"/>
                        <operator name="IS_SEL"/>
                        <operator name="OR"/>            
                    </condition>
                    <then>
                        <transition target="s4"/>
                    </then>
                    <else>
                        <transition target="p2"/>
                    </else>
                </if>
            </state>
            <state id="s4">
                <variable ref="Q4"/>
                <transition target="p2"/>
            </state>
            <state id="p2">
                <if>
                    <condition>
                        <variable ref="Q2"/>
                        <constant type="string" value="yes"/>
                        <operator name="IS_SEL"/>
                    </condition>
                    <then>
                        <transition target="s5"/>
                    </then>
                    <else>
                        <transition target="p3"/>
                    </else>
                </if>
            </state>
            <state id="s5">
                <variable ref="Q5"/>
                <transition target="p3"/>
            </state>
            <state id="p3">
                <if>
                    <condition>
                        <variable ref="Q5"/>
                        <constant type="string" value="1"/>
                        <operator name="IS_SEL"/>
                        <variable ref="Q5"/>
                        <constant type="string" value="2"/>
                        <operator name="IS_SEL"/>
                        <operator name="OR"/>
                        <variable ref="Q2"/>
                        <constant type="string" value="no"/>
                        <operator name="IS_SEL"/>
                        <operator name="OR"/>   
                    </condition>
                    <then>
                        <transition target="s6"/>
                    </then>
                    <else>
                        <transition target="s7"/>
                    </else>
                </if>
            </state>
            <state id="s6">
                <variable ref="Q6"/>
                <transition target="s7"/>
            </state>
            <state id="s7">
                <variable ref="Q7a"/>
                <transition target="s8"/>
            </state>
            <state id="s8">
                <variable ref="Q7b"/>
                <transition target="s9"/>
            </state>
            <state id="s9">
                <variable ref="Q7c"/>
                <transition target="s10"/>
            </state>
            <state id="s10">
                <variable ref="Q8a"/>
                <transition target="s11"/>
            </state>
            <state id="s11">
                <variable ref="Q8b"/>
                <transition target="s12"/>
            </state>
            <state id="s12">
                <variable ref="Q8c"/>
                <transition target="p4"/>
            </state>
            <state id="p4">
                <if>
                    <condition>
                        <variable ref="QB"/>
                        <constant type="string" value="male"/>
                        <operator name="IS_SEL"/>
                        <variable ref="QA"/>
                        <constant type="string" value="scotland"/>
                        <operator name="IS_SEL"/>
                        <variable ref="QA"/>
                        <constant type="string" value="wales"/>
                        <operator name="IS_SEL"/>
                        <operator name="OR"/>
                        <operator name="AND"/>
                        <variable ref="Q5"/>
                        <constant type="string" value="1"/>
                        <operator name="IS_SEL"/>
                        <variable ref="Q5"/>
                        <constant type="string" value="2"/>
                        <operator name="IS_SEL"/>
                        <operator name="OR"/>
                        <operator name="AND"/>
                    </condition>
                    <then>
                        <transition target="s13"/>
                    </then>
                    <else>
                        <transition target="s14"/>
                    </else>
                </if>
            </state>
            <state id="s13">
                <variable ref="Q9"/>
                <transition target="s14"/>
            </state>
            <state id="s14">
                <variable ref="Q10"/>
                <transition target="s15"/>
            </state>
            <state id="s15">
                <variable ref="END"/>
                <transition target="sink0"/>
            </state>
            <state id="sink0">
                <sink/>
            </state>
        </statemodel>
        <entrypoint>
            <source id="c0"/>
            <state id="c0">
                <include statemodel="DEFAULT"/>
                <transition target="sink0"/>
            </state>
            <state id="sink0">
                <sink/>
            </state>
        </entrypoint>
    </routing>
    <personalisation>
        <piping ref="DEFAULT">
            <pipe id="pipe0">
                <variable ref="Q7a"/>
                <operator name="UNSEL"/>
            </pipe>
            <pipe id="pipe1">
                <variable ref="Q7a"/>
                <operator name="SEL"/>
                <variable ref="Q7b"/>
                <operator name="SEL"/>
                <operator name="UNION"/>
            </pipe>
            <pipe id="pipe2">
                <variable ref="Q8a"/>
                <operator name="UNSEL"/>
            </pipe>
            <pipe id="pipe3">
                <variable ref="Q8a"/>
                <operator name="SEL"/>
                <variable ref="Q8b"/>
                <operator name="SEL"/>
                <operator name="UNION"/>        
            </pipe>
        </piping>
